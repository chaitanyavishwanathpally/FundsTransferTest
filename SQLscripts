


CREATE TABLE CUSTOMER_DTLS
(
  CUSTOMER_ID CHARACTER VARYING(100) NOT NULL,
  CUSTOMER_NAME CHARACTER VARYING(255) NOT NULL,
  BANK_IFSC CHARACTER VARYING(300),
  BANK_BRANCH CHARACTER VARYING(300),
  ACTIVE_YN CHARACTER VARYING(5),
  CRT_DATE TIMESTAMP WITHOUT TIME ZONE,
  CRT_USER CHARACTER VARYING(255),
  LST_UPD_DATE TIMESTAMP WITHOUT TIME ZONE,
  LST_UPD_USER CHARACTER VARYING(255),
  CONSTRAINT T_ACCOUNT_DTLS_PKEY PRIMARY KEY (CUSTOMER_ID)
);

CREATE TABLE T_ACCOUNT_DTLS
(
  ACCOUNT_NO CHARACTER VARYING(100) NOT NULL,
  CUSTOMER_ID CHARACTER VARYING(100) NOT NULL,
  ACCOUNT_TYPE CHARACTER VARYING(100) NOT NULL,
  BALANCE_AMT NUMERIC NOT NULL,
  UNCLEAR_AMT NUMERIC,
  ACTIVE_YN CHARACTER VARYING(5),
  CRT_DATE TIMESTAMP WITHOUT TIME ZONE,
  CRT_USER CHARACTER VARYING(255),
  LST_UPD_DATE TIMESTAMP WITHOUT TIME ZONE,
  LST_UPD_USER CHARACTER VARYING(255),
  CONSTRAINT T_ACCOUNT_BALANCE_DTLS_PKEY PRIMARY KEY (ACCOUNT_NO)
);

CREATE TABLE T_BANK_BOOK_DTLS
(
  BANK_BOOK_ID CHARACTER VARYING(100) NOT NULL,
  ACCOUNT_NO CHARACTER VARYING(100) NOT NULL,
  TRANSACTION_ID CHARACTER VARYING(100) NOT NULL,
  TRANSACTION_TYPE CHARACTER VARYING(100) NOT NULL,
  TRANSFER_AMT NUMERIC NOT NULL,
  BALANCE_AMT NUMERIC NOT NULL,
  ACTIVE_YN CHARACTER VARYING(5),
  CRT_DATE TIMESTAMP WITHOUT TIME ZONE,
  CRT_USER CHARACTER VARYING(255),
  LST_UPD_DATE TIMESTAMP WITHOUT TIME ZONE,
  LST_UPD_USER CHARACTER VARYING(255),
  CONSTRAINT T_BANK_BOOK_DTLS_PKEY PRIMARY KEY (BANK_BOOK_ID)
);

CREATE TABLE T_ACCOUNT_TRANSACTION_DTLS
(
  TRANSACTION_ID CHARACTER VARYING(100) NOT NULL,
  DEBIT_ACCOUNT_NO CHARACTER VARYING(100) NOT NULL,
  CREDIT_ACCOUNT_NO CHARACTER VARYING(100) NOT NULL,
  DESCRIPTION CHARACTER VARYING(255) NOT NULL,
  TRANSFER_MODE CHARACTER VARYING(300),
  TRANSFER_AMT NUMERIC,
  STATUS CHARACTER VARYING(300),
  ACTIVE_YN CHARACTER VARYING(5),
  CRT_DATE TIMESTAMP WITHOUT TIME ZONE,
  CRT_USER CHARACTER VARYING(255),
  LST_UPD_DATE TIMESTAMP WITHOUT TIME ZONE,
  LST_UPD_USER CHARACTER VARYING(255),
  CONSTRAINT T_ACCOUNT_TRANSACTION_DTLS_PKEY PRIMARY KEY (TRANSACTION_ID)
);

CREATE SEQUENCE BANK_BOOK_DTLS_SEQ
START 1;

CREATE SEQUENCE TRANSACTION_DTLS_SEQ
START 100000000000;

insert into CUSTOMER_DTLS values('100000','V Chaitanya','IFSC0000000','HYDERABAD','Y',current_timestamp,'ADMIN',current_timestamp,'ADMIN');
insert into CUSTOMER_DTLS values('100001','Srini','IFSC0000000','HYDERABAD','Y',current_timestamp,'ADMIN',current_timestamp,'ADMIN');

insert into T_ACCOUNT_DTLS values('1000000000','100000','SAVINGS',50000.00,0.00,'Y',current_timestamp,'ADMIN',current_timestamp,'ADMIN');
insert into T_ACCOUNT_DTLS values('1000000001','100001','SAVINGS',20000.00,0.00,'Y',current_timestamp,'ADMIN',current_timestamp,'ADMIN');
